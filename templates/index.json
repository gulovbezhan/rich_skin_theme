    "testimonials": {
      "type": "testimonials",
      "blocks": {
        "t1": {
          "type": "testimonial",
          "settings": {
            "icon": "quote",
            "testimonial": "<p>Love my new facial lift bar! After using it for 1 month, my skin has visibly improved. I use it for 10 minutes every morning and night — the results speak for themselves.<\/p>",
